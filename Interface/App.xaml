<Application x:Class="Interface.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Interface"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <Style x:Key="FirstFont" TargetType="TextBlock">
            <Setter Property="FontFamily" Value="Fonts/Italiana-Regular.ttf #Italiana"></Setter>
        </Style>
        <Style x:Key="SecondFont" TargetType="TextBlock">
            <Setter Property="FontFamily" Value="Fonts/Kameron-Bold.ttf #Kameron"></Setter>
        </Style>
        <FontFamily x:Key="Italiana-Regular"></FontFamily>
        <FontFamily x:Key="Kameron-Bold"></FontFamily>
    </Application.Resources>

</Application>
